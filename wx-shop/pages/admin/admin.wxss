/*admin.css  */

.admin-wrap{

}
.admin-top{
  height: 200rpx;
  background-color: #fff;
  width: 100%;
  margin-top: 20rpx;
  display: flex;
  align-items: center;
  padding: 0 30rpx;
}
.admin-top-left{
  flex: 0 0 160rpx;
  height: 160rpx;
  background:url(data:image/svg+xml;base64,) no-repeat;
  background-size: contain
}
.admin-top-middle{
  flex: 0 0 440rpx;
  height: 160rpx;
  padding-top: 35rpx;
  margin-left: 30rpx
}
.admin-top-right{
  width: 60rpx;
  height: 60rpx;
  background-image: url(data:image/svg+xml;base64,);
  background-repeat: no-repeat;
  background-size: contain
}
.bind-mi{
  width: 240rpx;
  height: 60rpx;
  text-align: center;
  line-height: 60rpx;
  background-color: #e1e1e1;
  border-radius: 10rpx;
  margin-top: 20rpx;
  font-size: 28rpx
}
.admin-style{
  height: 130rpx;
  display: flex;
  background-color: #fff;
  margin-top: 20rpx;
  color: #777
}
.admin-oder-li>.admin-style-order{
  height: 130rpx;
  display: flex;
  background-color: #fff;
  color: #777;
  border-bottom: 1px solid #ccc;
}
.admin-mifans-icon{
  background: url(data:image/svg+xml;base64,) no-repeat;
}
.admin-set-icon{
  background: url(data:image/svg+xml;base64,) no-repeat;
}
.admin-mifans-text{
  box-sizing: border-box;
  width: 90%;
  height: 100%;
  display: flex;
  align-items: center;
  background-position: 10rpx;
  margin: 0 auto;
  padding-left: 90rpx;
  background-size:60rpx 60rpx;
}
.admin-order{
  background-color: #fff;
  margin-top: 20rpx;
}
